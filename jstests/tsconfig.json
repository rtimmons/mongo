{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "declaration": false,
    "noImplicitAny": false,
    "lib": ["es2017"],
    "module": "CommonJS",
    "rootDir": ".",
    "outDir": "tsbuild",
    "target": "es2017",
    "moduleResolution": "node",
    "typeRoots": [
      "./mytypes"
    ],
    "traceResolution": true
  },
  "include": [
      "concurrency/fsm_workload_helpers/server_types.js",
      "libs/check_log.js",
      "libs/write_concern_util.js"
  ]
}
